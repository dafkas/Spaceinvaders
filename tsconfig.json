{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noEmit": true,
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "out": "docs/js/main.js",
  },
  "include": [
    "**/*"
  ],
  "exclude": [
    "assets",
    "node_modules",
    "docs",
  ],
}